<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>dev-sam</application>
    <version>2</version>
    <runtime>java8</runtime>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>

    <static-files>
        <include path="/**.png" />
        <include path="/**.jpg" />
        <include path="/**.html" />
        <include path="/**.js" />
        <include path="/**.ico" />
        <include path="/**.gif" />
        <include path="/**.css" />
        <include path="/**.zip" />
        <include path="/**.pdf" />
    </static-files>
</appengine-web-app>
